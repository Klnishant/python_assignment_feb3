   "source": [
    "num = int(input(\"Enter the number to check palindrome or not\"))\n",
    "reverse=0\n",
    "\n",
    "while num>0 :\n",
    "    digit = num%10\n",
    "    reverse=(reverse*10)+digit\n",
    "    digit=digit/10\n",
    "    \n",
    "if reverse == num :\n",
    "    print(\"it is pallindrome\")\n",
    "else :\n",
    "    print(\"it is not apallindrome\")"
   ]
